
<h3>Log In</h3>
<%= form_tag url: "/user/login", method: 'GET' do %>
  <%= label_tag(:phone_number, "Phone Number: ") %>
  <%= text_field_tag(:phone_number) %>
  <%= label_tag(:password, "Password: ") %>
  <%= password_field_tag(:password) %>
  <%= submit_tag("Log In") %>
<% end %>


<a href='user/new'><h5>Sign Up</h5></a>